<div class="container-fluid first-section">
  <div class="row h1-tag"><h1>Curation Top Picks</h1></div>
  <% @curation.art_objects.each do |object| %>
    <%= link_to(art_object_path(object.id)) do %>
      <div class="row object-list">
        <div class="col-xs-3">
          <img src='<%= object["PrimaryImage"]["Tiny"] %>' />
        </div>
        <div class="col-xs-9">
          <%= "[ " + object.score.to_s + " ]  " + object["Title"] %>
        </div>
      </div>
    <% end %>
  <% end %>
</div>