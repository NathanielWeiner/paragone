<div class="container-fluid first-section">
  <div class="row h1-tag"><h1><%= @curation.title %> Top Picks</h1></div>
  <% @art_objects.each do |object| %>
    <%= link_to(art_object_path(object["ObjectID"])) do %>
      <div class="row object-list">
        <div class="col-xs-3">
          <img src='<%= object["Images"][0]["ImageURLs"]["Medium"] %>' />
        </div>
        <div class="col-xs-9">
          <%= object["Title"] %>
        </div>
      </div>
    <% end %>
  <% end %>
</div>