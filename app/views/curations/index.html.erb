<div class="container-fluid first-section">
  <div class="row h1-tag"><h1>Curation Top Picks</h1></div>
  <% @curation.each do |object| %>
    <div class="row object-list">
      <div class="col-xs-3">
      </div>
      <div class="col-xs-5">
        <%= link_to object["title"], curation_path(object["id"]) %>
      </div>
      <div class="col-xs-4">
        <%= object["description"] %>
      </div>
    </div>
  <% end %>
</div>